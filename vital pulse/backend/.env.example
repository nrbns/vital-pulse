# Server Configuration
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/pulse_dev

# Redis
REDIS_URL=redis://localhost:6379
REDIS_ENABLED=false

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# Twilio SMS (Optional)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=

# Firebase Cloud Messaging (Optional)
FCM_SERVER_KEY=
FCM_PROJECT_ID=

# Google Maps API (Optional)
GOOGLE_MAPS_API_KEY=

# PostgreSQL Listener (Realtime)
ENABLE_PG_LISTENER=true

# CORS
CORS_ORIGIN=http://localhost:19006,http://localhost:3000
